      <Query>
        <DataSourceName>posConnectionString</DataSourceName>
        <CommandText>SELECT        department_name, address, date_format(rtnpr.return_date, '%d/%m/%Y') as return_date, rtnpr.purchase_order_id, rtnpr.purchase_order_detail_id, type_name, pordd.product_id, product_name, color_name, size_name, SUM(rtnpr.quantity) AS quantity, pordd.price 
FROM            return_po rtnpr INNER JOIN
                         purchase_order pord ON pord.purchase_order_id = rtnpr.purchase_order_id INNER JOIN
                         purchase_order_detail pordd ON pordd.purchase_order_detail_id = rtnpr.purchase_order_detail_id AND 
                         pord.purchase_order_id = pordd.purchase_order_id INNER JOIN
                         allproduct pr ON pr.product_id = pordd.product_id
left join department dp on dp.department_id=pord.department_id 
WHERE        (rtnpr.return_date &gt;= @FromDate) AND (rtnpr.return_date &lt;= @ToDate) AND (pord.department_id = @deptID)
GROUP BY rtnpr.return_date,  type_name, product_name, color_name, size_name
order by rtnpr.return_date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
