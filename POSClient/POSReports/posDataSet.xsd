          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="StockStatisticTableAdapter" GeneratorDataComponentClassName="StockStatisticTableAdapter" Name="stockStatistic" UserDataComponentName="StockStatisticTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="posConnectionString (Settings)" DbObjectName="pos.allproduct" DbObjectType="View" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select type_name, pr.product_id, product_name, color_name, size_name, sum(ifnull(good_qty,0)) as prestk_qty,
sum(ifnull(instock_qty,0)) as instock_qty, sum(ifnull(error_qty,0)) as error_qty, sum(ifnull(dmg_qty,0)) as dmg_qty,
sum(ifnull(instkout.quantity,0)) as stkout_qty, sum(ifnull(tmpout.quantity,0)) as tmpout_qty, sum(ifnull(sprtn.quantity,0)) as rtn_qty,
sum(ifnull(dmg.quantity,0)) as destroy_qty, sum(ifnull(good_qty,0)+ifnull(instock_qty,0)-ifnull(tmpout.quantity,0)-ifnull(dmg_qty,0)-ifnull(instkout.quantity,0)-ifnull(sprtn.quantity,0)-ifnull(dmg.quantity,0)) as realstock
from allproduct pr
    left join (select product_id, sum(good_count) as good_qty from stock_history where create_date&lt;@FromDate
      group by product_id) as stk on pr.product_id=stk.product_id
    left join (select product_id, sum(error_quantity) as error_qty,
      sum(damage_quantity+lost_quantity) as dmg_qty from stock
      where create_date&gt;=@FromDate and create_date&lt;=@ToDate group by product_id) as prestock
    on prestock.product_id=pr.product_id
    left join (select product_id, sum(quantity) as instock_qty from stock_in inner join
      stock_in_detail stkdt on stkdt.stock_in_id=stock_in.stock_in_id
      where stock_in_date&gt;=@FromDate and stock_in_date&lt;=@ToDate and stock_in.stock_in_type=0
      group by product_id) as instock on pr.product_id=instock.product_id
    left join (select product_id, sum(quantity) as quantity from department_stock_in dstk inner join
      department_stock_in_detail dstkd on dstk.stock_in_id=dstkd.stock_in_id
      where stock_in_date&gt;=@FromDate and stock_in_date&lt;=@ToDate and stock_in_type=0
      group by product_id) as instkout on instkout.product_id=pr.product_id
    left join (SELECT product_id, sum(quantity) as quantity from stock_out inner join
      stock_out_detail stkd on stock_out.stockout_id=stkd.stockout_id
      where stkd.defect_status_id=4 and stock_out_date&gt;=@FromDate and stock_out_date&lt;=@ToDate
      group by product_id) as tmpout on tmpout.product_id=pr.product_id
    left join (SELECT product_id, sum(quantity) as quantity from stock_out inner join
      stock_out_detail stkd on stock_out.stockout_id=stkd.stockout_id
      where stkd.defect_status_id=5 and stock_out_date&gt;=@FromDate and stock_out_date&lt;=@ToDate
      group by product_id) as sprtn on sprtn.product_id=pr.product_id
    left join (SELECT product_id, sum(quantity) as quantity from stock_out inner join
      stock_out_detail stkd on stock_out.stockout_id=stkd.stockout_id
      where stkd.defect_status_id=8 and stock_out_date&gt;=@FromDate and stock_out_date&lt;=@ToDate
      group by product_id) as dmg on dmg.product_id=pr.product_id
group by type_name, product_name, color_name, size_name</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="FromDate" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="DateTime" Direction="Input" ParameterName="@FromDate" Precision="0" ProviderType="DateTime" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
