          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SupplierStockoutTableAdapter" GeneratorDataComponentClassName="SupplierStockoutTableAdapter" Name="supplierStockout" UserDataComponentName="SupplierStockoutTableAdapter">
