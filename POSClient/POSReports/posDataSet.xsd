                            FROM          department_stock_out dpout INNER JOIN
                                                   department_stock_out_detail stkd ON dpout.STOCK_OUT_ID = stkd.STOCK_OUT_ID 
                                                   left join department dpt on dpt.department_id=dpout.department_id
                            WHERE      (stkd.DEFECT_STATUS_ID = 4) AND (dpout.STOCK_OUT_DATE &lt;= @ToDate) AND 
