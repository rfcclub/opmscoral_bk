                            FROM          department_stock_out dpout INNER JOIN
                                                   department_stock_out_detail stkd ON dpout.STOCK_OUT_ID = stkd.STOCK_OUT_ID
                            WHERE      (stkd.DEFECT_STATUS_ID = 6) AND (dpout.STOCK_OUT_DATE &gt;= @FromDate) AND (dpout.STOCK_OUT_DATE &lt;= @ToDate) AND 
                                                   (dpout.DEPARTMENT_ID = @deptID)
                            GROUP BY stkd.PRODUCT_ID) sprtn ON sprtn.PRODUCT_ID = pr.product_id LEFT OUTER JOIN
                          (SELECT     stkd.PRODUCT_ID, SUM(stkd.QUANTITY) AS quantity
                            FROM          department_stock_out dpout INNER JOIN
                                                   department_stock_out_detail stkd ON dpout.STOCK_OUT_ID = stkd.STOCK_OUT_ID
                            WHERE      (stkd.DEFECT_STATUS_ID = 8) AND (dpout.STOCK_OUT_DATE &gt;= @FromDate) AND (dpout.STOCK_OUT_DATE &lt;= @ToDate) AND 
                                                   (dpout.DEPARTMENT_ID = @deptID)
                            GROUP BY stkd.PRODUCT_ID) dmg ON dmg.PRODUCT_ID = pr.product_id LEFT OUTER JOIN
                          (SELECT     rtn.RETURN_DATE, rtn.purchase_order_id, rtn.PRODUCT_ID, SUM(rtn.QUANTITY) AS quantity
                            FROM          return_po rtn
                            WHERE      (rtn.RETURN_DATE &gt;= @FromDate) AND (rtn.RETURN_DATE &lt;= @ToDate) AND (rtn.DEPARTMENT_ID = @deptID)
                            GROUP BY rtn.RETURN_DATE, rtn.PRODUCT_ID) bkpro ON bkpro.PRODUCT_ID = pr.product_id
GROUP BY pr.type_name, pr.product_name, pr.color_name, pr.size_name</CommandText>
