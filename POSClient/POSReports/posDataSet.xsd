                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select type_name, pr.product_id, product_name, color_name, size_name, sum(ifnull(stk.good_qty,0)) as preqty,
  sum(instock.instock_qty) as inqty, sum(ifnull(stkout.outstock_qty,0)) as outqty, sum(ifnull(dstk.good_qty,0)) as dpreqty,
  sum(ifnull(dinstock.instock_qty,0)) as dinstock, sum(ifnull(dstkout.outstock_qty,0)) as doutstock
  from allproduct pr

left join (SELECT  A.PRODUCT_ID, A.GOOD_COUNT AS good_qty
                            FROM          stock_history A
                            WHERE    A.CREATE_DATE = ( SELECT max(b.CREATE_DATE)
                                       FROM stock_history b where
                                       (b.CREATE_DATE &lt;@FromDate ) AND b.Product_id = a.Product_ID
                                       GROUP BY b.product_id

                            )
                            GROUP BY a.PRODUCT_ID ) as stk on pr.product_id=stk.product_id

    left join (select product_id, sum(quantity) as instock_qty from stock_in inner join
      stock_in_detail stkdt on stkdt.stock_in_id=stock_in.stock_in_id
      where stock_in_date&gt;=@FromDate and stock_in_date&lt;=@ToDate
      group by product_id) as instock on pr.product_id=instock.product_id

left join (select product_id, sum(quantity) as outstock_qty from stock_out inner join
      stock_out_detail stkdt on stkdt.stockout_id=stock_out.stockout_id
      where stock_out_date&gt;=@FromDate and stock_out_date&lt;=@ToDate
      group by product_id) as stkout on stkout.product_id=pr.product_id

left join (SELECT  A.PRODUCT_ID, A.GOOD_COUNT AS good_qty
                            FROM          department_stock_history A
                            WHERE    A.CREATE_DATE = ( SELECT max(b.CREATE_DATE)
                                       FROM department_stock_history b where
                                       (b.CREATE_DATE &lt;@FromDate ) AND b.Product_id = a.Product_ID
                                       GROUP BY b.product_id)
                            GROUP BY a.PRODUCT_ID ) as dstk on pr.product_id=dstk.product_id
left join (select product_id, sum(quantity) as instock_qty from department_stock_in inner join
      department_stock_in_detail stkdt on stkdt.stock_in_id=department_stock_in.stock_in_id
      where stock_in_date&gt;=@FromDate and stock_in_date&lt;=@ToDate
      group by product_id) as dinstock on pr.product_id=dinstock.product_id

left join (select product_id, sum(quantity) as outstock_qty from department_stock_out dstkout inner join
      department_stock_out_detail stkdt on stkdt.stock_out_id=dstkout.stock_out_id
      where stock_out_date&gt;=@FromDate and stock_out_date&lt;=@ToDate
      group by product_id) as dstkout on stkout.product_id=pr.product_id
group by type_name, product_name, color_name, size_name</CommandText>
