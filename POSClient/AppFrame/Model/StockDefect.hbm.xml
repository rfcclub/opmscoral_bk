<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AppFrame" namespace="AppFrame.Model">

<class name="StockDefect"
	    table="stock_defect"
>

    <id
        name="StockDefectId"
        type="Int64"
        column="STOCK_DEFECT_ID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="Quantity"
        type="Int64"
        column="QUANTITY"
        length="10"
    >
    </property>
    <property
        name="CreateDate"
        type="DateTime"
        column="CREATE_DATE"
        length="19"
    >
    </property>
    <property
        name="CreateId"
        type="string"
        column="CREATE_ID"
        length="50"
    >
    </property>
    <property
        name="UpdateDate"
        type="DateTime"
        column="UPDATE_DATE"
        length="19"
    >
    </property>
    <property
        name="UpdateId"
        type="string"
        column="UPDATE_ID"
        length="50"
    >
    </property>
    <property
        name="ExclusiveKey"
        type="Int64"
        column="EXCLUSIVE_KEY"
        length="10"
    >
    </property>
    <property
        name="DelFlg"
        type="Int64"
        column="DEL_FLG"
        length="8"
    >
    </property>

  <property
      name="Description"
      type="string"
      column="DESCRIPTION"
      length="200"
    >
  </property>
  
  <property
      name="GoodCount"
      type="Int64"
      column="GOOD_COUNT"
      length="10"
    >
  </property>
  <property
      name="ErrorCount"
      type="Int32"
      column="ERROR_COUNT"
      length="8"
    >
  </property>
  <property
      name="DamageCount"
      type="Int32"
      column="DAMAGE_COUNT"
      length="8"
    >
  </property>
  <property
      name="LostCount"
      type="Int32"
      column="LOST_COUNT"
      length="8"
    >
  </property>
  <property
      name="UnconfirmCount"
      type="Int32"
      column="UNCONFIRM_COUNT"
      length="8"
    >
  </property>
    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Product -->
    <many-to-one
        name="Product"
        class="Product"
        not-null="true"
        lazy="false"
    >
      <column name="PRODUCT_ID" />
    </many-to-one>

  <many-to-one
      name="ProductMaster"
      class="ProductMaster"
      not-null="true"
    >
    <column name="PRODUCT_MASTER_ID" />
  </many-to-one>
  <many-to-one
      name="Stock"
      class="Stock"
      not-null="true"
    >
    <column name="STOCK_ID" />
  </many-to-one>
  
</class>
</hibernate-mapping>
