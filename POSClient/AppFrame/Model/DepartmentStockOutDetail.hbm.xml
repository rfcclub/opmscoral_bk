  <id
       name="StockOutDetailId"
       type="Int64"
       column="STOCK_OUT_DETAIL_ID"
    >
    <generator class="assigned" />
  </id>
    <property
        name="Quantity"
        type="Int64"
        column="QUANTITY"
        length="10"
    >
    </property>
    <property
        name="Description"
        type="string"
        column="DESCRIPTION"
        length="5000"
    >
    </property>
    <property
        name="CreateDate"
        type="DateTime"
        column="CREATE_DATE"
        length="19"
    >
    </property>
    <property
        name="CreateId"
        type="string"
        column="CREATE_ID"
        length="50"
    >
    </property>
    <property
        name="UpdateDate"
        type="DateTime"
        column="UPDATE_DATE"
        length="19"
    >
    </property>
    <property
        name="UpdateId"
        type="string"
        column="UPDATE_ID"
        length="50"
    >
    </property>
    <property
        name="ExclusiveKey"
        type="Int64"
        column="EXCLUSIVE_KEY"
        length="10"
    >
    </property>
    <property
        name="DelFlg"
        type="Int64"
        column="DEL_FLG"
        length="10"
    >
    </property>

    <!-- Associations -->
    <!-- derived association(s) for compound key -->
    <!-- bi-directional many-to-one association to DepartmentStockOut -->
    <many-to-one
        name="DepartmentStockOut"
	    class="DepartmentStockOut"	    
	>
    	<column name="STOCK_OUT_ID" />
    	<column name="DEPARTMENT_ID" />
	</many-to-one>
	
    <!-- bi-directional many-to-one association to Product -->
    <many-to-one
        name="Product"
	    class="Product"	 
      lazy="false"   
	>
    	<column name="PRODUCT_ID" />
	</many-to-one>
	
    <!-- end of derived association(s) -->

  <many-to-one
  name="ProductMaster"
class="ProductMaster"
lazy="false"
	>
    <column name="PRODUCT_MASTER_ID" />
  </many-to-one>
  
  <property
         name="GoodQuantity"
         type="Int64"
         column="GOOD_QUANTITY"
         length="10"
    >
  </property>
  <property
        name="ErrorQuantity"
        type="Int64"
        column="ERROR_QUANTITY"
        length="10"
    >
  </property>
  <property
        name="LostQuantity"
        type="Int64"
        column="LOST_QUANTITY"
        length="10"
    >
  </property>
  <property
        name="UnconfirmQuantity"
        type="Int64"
        column="UNCONFIRM_QUANTITY"
        length="10"
    >
  </property>
