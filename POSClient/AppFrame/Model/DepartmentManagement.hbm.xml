<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AppFrame" namespace="AppFrame.Model">

<class name="DepartmentManagement"
	    table="department_management"
>

  <composite-id name="DepartmentManagementPK" class="DepartmentManagementPK">
    <key-property
        name="DepartmentId"
        column="DEPARTMENT_ID"
        type="Int64"
        length="10">
    </key-property>
    <key-property
        name="EmployeeId"
        column="EMPLOYEE_ID"
        type="string"
        length="20">
    </key-property>
  </composite-id>

    <property
        name="Position"
        type="long"
        column="POSITION"        
    >
    </property>
    <property
        name="CreateDate"
        type="DateTime"
        column="CREATE_DATE"
        length="19"
    >
    </property>
    <property
        name="CreateId"
        type="string"
        column="CREATE_ID"
        length="50"
    >
    </property>
    <property
        name="UpdateDate"
        type="DateTime"
        column="UPDATE_DATE"
        length="19"
    >
    </property>
    <property
        name="UpdateId"
        type="string"
        column="UPDATE_ID"
        length="50"
    >
    </property>

  <property
        name="WorkingDay"
        type="DateTime"
        column="WORKING_DAY"
        length="19"
    >
  </property>
  <property
        name="StartTime"
        type="DateTime"
        column="START_TIME"
        length="19"
    >
  </property>
  <property
        name="EndTime"
        type="DateTime"
        column="END_TIME"
        length="19"
    >
  </property>
</class>
</hibernate-mapping>
