<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AppFrame" namespace="AppFrame.Model">

<class name="StockDefectStatus"
	    table="stock_defect_status"
>

    <id
        name="DefectStatusId"
        type="Int64"
        column="DEFECT_STATUS_ID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="DefectStatusName"
        type="string"
        column="DEFECT_STATUS_NAME"
        length="200"
    >
    </property>    

</class>
</hibernate-mapping>
