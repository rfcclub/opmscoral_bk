<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AppFrame" namespace="AppFrame.Model">

<class name="StockOut"
	    table="stock_out"
>

    <id
        name="StockoutId"
        type="Int64"
        column="STOCKOUT_ID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="StockOutDate"
        type="DateTime"
        column="STOCK_OUT_DATE"
        length="19"
    >
    </property>
    <property
        name="DepartmentId"
        type="long"
        column="DEPARTMENT_ID"
        not-null="true"
        length="10"
    >
    </property>
    <property
        name="CreateDate"
        type="DateTime"
        column="CREATE_DATE"
        length="19"
    >
    </property>
    <property
        name="CreateId"
        type="string"
        column="CREATE_ID"
        length="50"
    >
    </property>
    <property
        name="UpdateDate"
        type="DateTime"
        column="UPDATE_DATE"
        length="19"
    >
    </property>
    <property
        name="UpdateId"
        type="string"
        column="UPDATE_ID"
        length="50"
    >
    </property>
    <property
        name="ExclusiveKey"
        type="Int64"
        column="EXCLUSIVE_KEY"
        length="10"
    >
    </property>
    <property
        name="StockId"
        type="Int64"
        column="STOCK_ID"
        length="10"
      >
    </property>
    <property
        name="DelFlg"
        type="Int64"
        column="DEL_FLG"
        length="10"
    >
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to StockOutDetail -->
    <bag
        name="StockOutDetails"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="STOCKOUT_ID" />
        </key>
        <one-to-many 
            class="StockOutDetail"
        />
    </bag>
    <!-- bi-directional one-to-many association to StockOutCost -->
    <bag
        name="StockOutCosts"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="STOCKOUT_ID" />
        </key>
        <one-to-many 
            class="StockOutCost"
        />
    </bag>

</class>
</hibernate-mapping>
