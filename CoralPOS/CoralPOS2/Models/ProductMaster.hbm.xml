<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="CoralPOS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CoralPOS.Models.ProductMaster, CoralPOS.Models" table="CRL_PRD_MST" lazy="true" >
    <id name="ProductMasterId" type="String" column="PRODUCT_MASTER_ID">
      <generator class="assigned" />
    </id>
    <property name="Barcode" column="BARCODE" />
    <property name="CreateDate" column="CREATE_DATE" />
    <property name="CreateId" column="CREATE_ID" />
    <property name="DelFlg" column="DEL_FLG" />
    <property name="Description" column="DESCRIPTION" />
    <property name="DistributorId" column="DISTRIBUTOR_ID" />
    <property name="ExFld1" column="EX_FLD1" />
    <property name="ExFld2" column="EX_FLD2" />
    <property name="ExFld3" column="EX_FLD3" />
    <property name="ExFld4" column="EX_FLD4" />
    <property name="ExFld5" column="EX_FLD5" />
    <property name="ExclusiveKey" column="EXCLUSIVE_KEY" />
    <property name="ImagePath" column="IMAGE_PATH" />
    <property name="ManufacturerId" column="MANUFACTURER_ID" />
    <property name="PackagerId" column="PACKAGER_ID" />
    <property name="ProductName" column="PRODUCT_NAME" />
    <property name="UnitId" column="UNIT_ID" />
    <property name="UpdateDate" column="UPDATE_DATE" />
    <property name="UpdateId" column="UPDATE_ID" />
    <many-to-one lazy="true" update="false" insert="false" class="ProductType">
      <column name="TYPE_ID" property-ref="TypeId" />
    </many-to-one>
    <many-to-one lazy="true" update="false" insert="false" class="Category">
      <column name="CATEGORY_ID" property-ref="CategoryId" />
    </many-to-one>
    <many-to-one lazy="true" update="false" insert="false" class="Country">
      <column name="COUNTRY_ID" property-ref="CountryId" />
    </many-to-one>
    <many-to-one lazy="true" update="false" insert="false" class="Supplier">
      <column name="SUPPLIER_ID" property-ref="SupplierId" />
    </many-to-one>
    <many-to-one lazy="true" update="false" insert="false" class="ProductUnit" />
  </class>
</hibernate-mapping>