<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="CoralPOS" namespace="CoralPOS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Product" table="CRL_PRD" >
    <id name="ProductId" type="String" column="PRODUCT_ID">
      <generator class="assigned" />
    </id>
    <property name="Barcode" column="BARCODE" />
    <property name="BlockDetailId" column="BLOCK_DETAIL_ID" />
    <property name="BlockId" column="BLOCK_ID" />
    <property name="CreateDate" column="CREATE_DATE" />
    <property name="CreateId" column="CREATE_ID" />
    <property name="DelFlg" column="DEL_FLG" />
    <property name="ExFld3" column="EX_FLD3" />
    <property name="ExFld4" column="EX_FLD4" />
    <property name="ExFld5" column="EX_FLD5" />
    <property name="ExclusiveKey" column="EXCLUSIVE_KEY" />
    <property name="Price" column="PRICE" />
    <property name="Quantity" column="QUANTITY" />
    <property name="TaxId" column="TAX_ID" />
    <property name="UpdateDate" column="UPDATE_DATE" />
    <property name="UpdateId" column="UPDATE_ID" />
    <many-to-one class="ProductMaster" name="ProductMaster">
      <column name="PRODUCT_MASTER_ID" />
    </many-to-one>
    <many-to-one class="ExProductColor" name="ProductColor" lazy="false">
      <column name="EX_FLD1" />
    </many-to-one>
    <many-to-one class="ExProductSize" name="ProductSize" lazy="false">
      <column name="EX_FLD2" />
    </many-to-one>
  </class>
</hibernate-mapping>