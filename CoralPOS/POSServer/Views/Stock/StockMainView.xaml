        <Button FontSize="14" FontWeight="Bold" Grid.Row="1" Height="88" HorizontalContentAlignment="Stretch" Margin="62,122.5,58,0" Name="AfterInventoryCollectorProcessing" TabIndex="9" VerticalAlignment="Top" VerticalContentAlignment="Center" Grid.Column="2">
