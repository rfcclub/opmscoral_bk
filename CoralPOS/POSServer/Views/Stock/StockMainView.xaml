        </Button>
        <TextBlock Name="textBlock1" Text="NHẬP HÀNG" TextAlignment="Center" FontSize="16" TextWrapping="Wrap" FontWeight="Normal" />
        <TextBlock Name="textBlock2" Grid.Column="1" Text="XUẤT HÀNG" TextAlignment="Center" FontSize="16" TextWrapping="Wrap" FontWeight="Normal" />
        <TextBlock Name="textBlock3" Grid.Column="2" Text="TÌM KIẾM VÀ KIỂM KÊ" TextAlignment="Center" FontSize="16" TextWrapping="Wrap" FontWeight="Normal" />
        <TextBlock Name="textBlock4" Grid.Column="3" Text="XÁC NHẬN" TextAlignment="Center" FontSize="16" TextWrapping="Wrap" FontWeight="Normal" />
    </Grid>
