  <object id="LoginFlow" type="AppFrame.Base.DefaultFlow, AppFrame" singleton="false">
    <property name="FlowSteps">
      <dictionary>
        <!-- Show login form -->
        <entry key="Step1" value="POSServer.ViewModels.Security.ILoginViewModel"/>
        <!-- run register action -->
        <entry key="Step2" value="POSServer.Actions.Security.IRegisterUserAction"/>
      </dictionary>
    </property>
    <property name="Session" ref="FlowSession" />
  </object>
  
