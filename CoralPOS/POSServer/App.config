    </parsers>
    <context>
      <resource uri="assembly://POSServer.DataLayer/POSServer.DataLayer/DataLayer.xml"/>
      <resource uri="assembly://POSServer.BusinessLogic/POSServer.BusinessLogic/BusinessLayer.xml"/>      
      <resource uri="assembly://POSServer/POSServer/ServerActions.xml"/>
      <resource uri="assembly://POSServer/POSServer/ServerViewModels.xml"/>
      <resource uri="assembly://POSServer/POSServer/spring-config.xml"/>
    </context>
