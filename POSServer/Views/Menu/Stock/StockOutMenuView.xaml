<UserControl x:Class="POSServer.Views.Menu.Stock.StockOutMenuView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="700" Width="174">
    <Grid>
        <Button FontSize="14" FontWeight="Bold" Height="80" Margin="50,12,45,0" Name="StockOutToDepartment" TabIndex="9" VerticalAlignment="Top">
            <TextBlock TextAlignment="Center" TextWrapping="Wrap">XUẤT HÀNG ĐẾN CỬA HÀNG</TextBlock>
        </Button>
        <Button FontSize="14" FontWeight="Bold" Height="80" HorizontalContentAlignment="Stretch" Margin="50,112,44,0" Name="StockOutToOther" TabIndex="9" VerticalAlignment="Top" VerticalContentAlignment="Center">
            <TextBlock TextAlignment="Center" TextWrapping="Wrap">XUẤT HÀNG ĐẾN NƠI KHÁC</TextBlock>
        </Button>
        <Button FontSize="14" FontWeight="Bold" Height="80" Margin="50,212,44,0" Name="StockOutList" TabIndex="9" VerticalAlignment="Top">
            <TextBlock TextAlignment="Center" TextWrapping="Wrap">DANH SÁCH LÔ HÀNG XUẤT</TextBlock>
        </Button>
        <Button FontSize="14" FontWeight="Bold" Margin="50,312,44,308" Name="StockOutByExcel" TabIndex="9">
            <TextBlock TextAlignment="Center" TextWrapping="Wrap" Width="66">XUẤT HÀNG BẰNG EXCEL</TextBlock>
        </Button>
    </Grid>
</UserControl>
