      </Fields>
      <Query>
        <DataSourceName>posConnectionString</DataSourceName>
        <CommandText>select dpt.department_name, address, type_name, pr.product_id, product_name, color_name, size_name, sum(quantity), sum(dstk.stock_in_cost)
from allproduct pr
  left join (department_stock_in dstk inner join department_stock_in_detail dstkd on dstk.stock_in_id=dstkd.stock_in_id) on dstkd.product_id=pr.product_id
  left join department dpt on dpt.department_id=dstk.department_id
where dstk.department_id=@deptID and dstk.stock_in_type=1 and stock_in_date&gt;=@FromDate and stock_in_date&lt;=@ToDate
group by type_name, product_name, color_name, size_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
