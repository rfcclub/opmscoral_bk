<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net"
        
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:appframe="http://www.appframe.com/schema/appframe"
	xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

  <object name="EmployeeLogicImpl" type="AppFrame.Logic.EmployeeLogicImpl, AppFrameClient" >
    <property name="EmployeeDAO" ref="AppFrame.DataLayer.IEmployeeDAO" />
  </object>
  <object name="EmployeeDetailLogicImpl" type="AppFrame.Logic.EmployeeDetailLogicImpl, AppFrameClient" >
    <property name="EmployeeDetailDAO" ref="AppFrame.DataLayer.IEmployeeDetailDAO" />
  </object>

  <object name="DepartmentLogicImpl" type="AppFrame.Logic.DepartmentLogicImpl, AppFrameClient" >
    <property name="DepartmentDAO" ref="AppFrame.DataLayer.IDepartmentDAO" />
  </object>
  
  
  
</objects>
